spring:
  application:
    name: order-service
  datasource:
    url: jdbc:postgresql://localhost:5432/hungerbox_orders
    username: postgres
    password: yourpassword
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true

server:
  port: 8081
eureka:
  client:
    service-url:
      defaultZone: http://localhost:8761/eureka/